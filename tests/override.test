- name: Make http request
  tags:
    - request
  preconditions:
    config_name: "config.http"
    config_files:
      config.http: |
        - request:
            path: /hello
          response:
            content: <html><body>correct1</body></html>

        - request:
            path: /hello
          response:
            content: <html><body>correct2</body></html>
  scenario:
    - Assert request response contains:
        url: http://localhost:10088/hello
        contains: correct1